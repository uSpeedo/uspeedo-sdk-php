{
    "name": "uspeedo/uspeedo-sdk-php",
    "type": "library",
    "description": "USpeedo",
    "keywords": ["php", "uspeedo"],
    "homepage": "https://github.com/uspeedo/uspeedo-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "uspeedo",
            "email": "support@uspeedo.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2.1|^7.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "phpcompatibility/php-compatibility": "^9.0"
    },
    "prefer-stable" : true,
    "autoload": {
        "psr-4" : {
            "USpeedo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "USpeedo\\Tests\\": "tests/" }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
